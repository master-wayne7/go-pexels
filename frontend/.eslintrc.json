{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "@typescript-eslint/no-unused-vars": "warn"
    },
    "overrides": [
        {
            "files": [
                "**/*.ts",
                "**/*.tsx"
            ],
            "rules": {
                "@typescript-eslint/no-explicit-any": "warn"
            }
        }
    ]
}